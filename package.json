    "inquirer": "^6.2.0",
    "mongoose": "^5.2.13",
    "morgan": "~1.9.0",
    "nodemailer": "^4.6.8",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
